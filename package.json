{
    "name": "@sparkleui/icons",
    "description": "Free, high quality, open source icon library based on Feathericons.",
    "version": "1.0.2",
    "type": "module",
    "author": "Sparkle UI",
    "license": "MIT",
    "browserslist": [
        "last 2 versions"
    ],
    "keywords": [
        "sparkle-ui",
        "sparkle",
        "icons",
        "feather-icons",
        "font",
        "svg",
        "woff",
        "woff2"
    ],
    "homepage": "https://sparkleui.github.io/sparkle/icons/introduction/what-is-sparkle-icons",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sparkleui/icons.git"
    },
    "bugs": {
        "url": "https://github.com/sparkleui/icons/issues"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "build-svg": "svgo -f ./src/icons/ --config svgo.config.js",
        "build-font": "fantasticon",
        "lint": "stylelint './src/scss/**/*.scss' --config .stylelintrc && prettier './src/scss/**/*.scss' --config .prettierrc --ignore-path .gitignore --check",
        "format": "stylelint './src/scss/**/*.scss' --config .stylelintrc --fix && prettier './src/scss/**/*.scss' --config .prettierrc --ignore-path .gitignore --write"
    },
    "devDependencies": {
        "@sparkleui/stylelint-config": "^1.0.0",
        "autoprefixer": "^10.4.16",
        "fantasticon": "^2.0.0",
        "prettier": "^3.1.1",
        "sass": "^1.69.5",
        "stylelint": "^16.1.0",
        "stylelint-prettier": "^5.0.0",
        "svgo": "^3.1.0",
        "vite": "^5.0.10"
    },
    "files": [
        "dist/*.css",
        "dist/fonts/*",
        "src/scss/*"
    ],
    "exports": {
        ".": "./dist/sparkle-icons.min.css",
        "./scss": "./src/scss/abstracts/_abstracts.scss"
    }
}
