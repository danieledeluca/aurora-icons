${{ name }}-prefix: '{{ prefix }}' !default;
${{ name }}-font-family: '{{ name }}' !default;
${{ name }}-directory: '{{ fontsUrl }}' !default;
${{ name }}-file: '#{${{ name }}-directory}/#{${{ name }}-font-family}' !default;
${{ name }}-font-src: {{{ fontSrc }}} !default;
${{ name }}: (
{{# each codepoints }}
    '{{ @key }}': '\\{{ codepoint this }}',
{{/ each }}
) !default;
